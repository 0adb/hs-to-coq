
# skip VarEnv.uniqAway'
in Combined.uniqAway' termination try deferred

# defined in midamble
# needs UniqFM.plusUFM_CD
skip Combined.plusVarEnv_CD

# needs Util.foldl2 
skip Combined.rnBndrs2

# add default constraint
redefine Definition Combined.lookupVarEnv_NF {a} `{_ : GHC.Err.Default a} (env : VarEnv a) id : a :=
      match lookupVarEnv env id with
      | Some xx => xx
      | None => GHC.Err.default
    end.


 