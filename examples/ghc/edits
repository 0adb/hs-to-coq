# Should skip these everywhere
skip module Outputable
skip module Panic
skip module GHC.Show


rename value Pair.Pair   = Pair.Mk_Pair
rename value SrcLoc.RealSrcSpan = SrcLoc.Mk_RealSrcSpan
rename value SrcLoc.RealSrcLoc  = SrcLoc.Mk_RealSrcLoc
rename value SrcLoc.SrcLoc      = SrcLoc.Mk_SrcLoc

axiomatize module UniqSet
