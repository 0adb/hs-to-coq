type  GHC.Tuple.() = unit
value GHC.Tuple.() = tt

type  GHC.Types.[] = list
value GHC.Types.[] = nil
value GHC.Types.:  = ::

value <  = <?
value <= = <=?
value >  = >?
value >= = >=?

type  GHC.Types.Bool  = bool
value GHC.Types.True  = true
value GHC.Types.False = false
value GHC.Types.not   = negb
value GHC.Types.||    = orb
value GHC.Types.&&    = andb


type  GHC.Base.Maybe   = option
value GHC.Base.Just    = Some
value GHC.Base.Nothing = None
