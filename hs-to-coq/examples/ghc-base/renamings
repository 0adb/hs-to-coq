type  () = unit
value () = tt

type  [] = list
value [] = nil
value :  = ::

# TODO: pairs???

type  Integer = Z

type  Bool  = bool
value True  = true
value False = false

type  Maybe   = option
value Just    = Some
value Nothing = None

